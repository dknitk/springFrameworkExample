<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean name="st1" class="com.kw.spring_core.primitive.Student">
		<property name="id">
			<value>101</value>
		</property>
		<property name="name">
			<value>Dharmendra</value>
		</property>
		<property name="address">
			<value>India</value>
		</property>
	</bean>
	
	<bean name="st2" class="com.kw.spring_core.primitive.Student">
		<property name="id" value="101">
		</property>
		<property name="name" value="Raju">
		</property>
		<property name="address" value="USA">
		</property>
	</bean>
	
	<bean name="st3" class="com.kw.spring_core.primitive.Student" p:id="103" p:name="Kumar" p:address="Kanpur"/>
		

</beans>